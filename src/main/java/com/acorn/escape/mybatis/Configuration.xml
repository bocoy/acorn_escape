<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.acorn.escape.notice.dto.NoticeDto"
			alias="noticeDto"/>
		<typeAlias type="com.acorn.escape.admin.dto.AdminDto"
			alias="adminDto"/>
		<typeAlias type="com.acorn.escape.confirm.dto.ConfirmDto"
			alias="confirmDto"/>
	</typeAliases>

	<mappers>
		<mapper resource="com/acorn/escape/mybatis/NoticeMapper.xml"/>
		<mapper resource="com/acorn/escape/mybatis/AdminMapper.xml"/>
		<mapper resource="com/acorn/escape/mybatis/ConfirmMapper.xml"/>
	</mappers>
</configuration>

